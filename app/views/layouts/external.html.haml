=render :partial => "layouts/header"
%body
  .container#wrapper
    %header#toolbar
      = link_to "CityVoice", dashboard_path, :id => "logo"
      %ul
        -#%li= link_to "Register", register_path
        %li= link_to "Sign In", login_path, :id => "login"
    =render :partial => "layouts/flash", :locals => { :flash => flash }
    %section#content.showgrid
      #yield
        = yield
        %hr.space
    %footer=render :partial => "layouts/footer"
    