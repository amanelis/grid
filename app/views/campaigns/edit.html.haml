- form_for @campaign, :url => channel_campaign_path, :method => "put" do |f|
  = f.label "Channel Name"
  %br
  = f.text_field :name
  %br
  %br
  = f.submit "Update"
  = link_to 'X', channel_campaign_path(@account, channel, campaign), :confirm => "This will delete this campaign, are you sure?", :method => :delete