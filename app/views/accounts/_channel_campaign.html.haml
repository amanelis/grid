%tr
  %td
    -#.edit= link_to 'Edit', edit_channel_campaign_path(@account, channel, campaign), :rel => "facebox" if can? :manipulate_campaign, campaign
    = link_to campaign.name, channel_campaign_path(@account, channel, campaign, :from => params[:from], :to => params[:to])
  %td= campaign.number_of_total_leads_between(cookies[:start_date], cookies[:end_date])
  %td= ActionController::Base.helpers.number_to_currency(campaign.cost_per_lead_between(cookies[:start_date], cookies[:end_date]))
